{
  "manifest_version": 3,
  "name": "Google Forms Extractor",
  "version": "1.0.0",
  "description": "Extracts entry names from Google Forms",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["https://docs.google.com/forms/*viewform*"],
  "action": {
    "default_popup": "./popup/popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "120": "images/icon120.png"
    }
  },
  "background": {
    "service_worker": "./background/background.js"
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "120": "images/icon120.png"
  }
}
